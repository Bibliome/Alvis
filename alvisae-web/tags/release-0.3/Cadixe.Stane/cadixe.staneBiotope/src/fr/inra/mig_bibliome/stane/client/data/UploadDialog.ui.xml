<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <g:VerticalPanel>
        <g:Label>Choose a document to upload</g:Label>
        <g:SimplePanel ui:field='uploadPanel'/>
        <g:HorizontalPanel>
        <g:Button text='Upload' ui:field='uploadButton' title='click to upload the selected file'/>
        <g:Button text='Cancel' ui:field='cancelButton' />
        </g:HorizontalPanel>
    </g:VerticalPanel>
</ui:UiBinder>
